    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="i, n, index" type="Integer" array="False" size=""/>
            <input variable="n"/>
            <declare name="a" type="Integer" array="True" size="n"/>
            <for variable="i" start="0" end="n-1" direction="inc" step="1">
                <output expression="&quot;a[&quot;&amp;i&amp;&quot;]:&quot;" newline="True"/>
                <input variable="a[i]"/>
            </for>
            <input variable="index"/>
            <for variable="i" start="index" end="n-1" direction="inc" step="1">
                <if expression="i = n-1">
                    <then>
                        <assign variable="n" expression="n-1"/>
                    </then>
                    <else>
                        <assign variable="index" expression="a[i]"/>
                        <assign variable="a[i]" expression="a[i+1]"/>
                    </else>
                </if>
            </for>
            <for variable="i" start="0" end="n-1" direction="inc" step="1">
                <output expression="&quot;a[&quot;&amp;i&amp;&quot;]:&quot;&amp;a[i]" newline="True"/>
            </for>
        </body>
    </function>
</flowgorithm>
